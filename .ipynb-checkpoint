    "# tu codigo aqui (~ 3 lineas de codigo)\n",
    "def calcular_entropia(px):\n",
    "    s = -np.nansum(px* np.log2(px))\n",
    "    return s\n",
    "\n",
    "print(calcular_entropia(x))\n"
   ]
